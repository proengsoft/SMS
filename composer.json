{
    "name": "Proengsoft/SMS",
    "description": "Paquete de laravel 5 para envio de SMS mediante el broker Altiria",
    "keywords": [
        "Proengsoft",
        "SMS"
    ],
    "homepage": "http://www.proengsoft.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Proengsoft",
            "email": "info@proengsoft.com",
            "homepage": "http://www.proengsoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
          "Proengsoft\\SMS\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Proengsoft\\SMS\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
